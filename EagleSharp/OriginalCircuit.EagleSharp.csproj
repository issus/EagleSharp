<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFramework>net6.0</TargetFramework>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Title>Eagle/Fusion360 Electronics File Format Library</Title>
	  <Authors>OriginalCircuit</Authors>
	  <Company>Original Circuit Limited</Company>
	  <Description>Provides a model for deserialising Eagle/Fusion360 Electronics files.</Description>
	  <Copyright>Copyright 2020-2023 Original Circuit Limited. Released under MIT License.</Copyright>
	  <PackageProjectUrl>https://github.com/issus/OriginalCircuit.EagleSharp</PackageProjectUrl>
	  <PackageReadmeFile>README.md</PackageReadmeFile>
	  <RepositoryUrl>https://github.com/issus/OriginalCircuit.EagleSharp</RepositoryUrl>
	  <PackageTags>eagle; electronics; fusion360; ecad; eda</PackageTags>
	  <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
